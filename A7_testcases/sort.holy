summon HolyScript

    invoke void sort(int[] arr, int length) {
        chant (int i = 0; i < length - 1; i++) {
            chant (int j = 0; j < length - i - 1; j++) {
                belief (arr[j] > arr[j + 1]) {
                    // Swap arr[j] and arr[j + 1]
                    int temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                }
            }
        }
    }

doom
